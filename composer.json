{
    "name": "beelab/user-password-bundle",
    "type": "symfony-bundle",
    "description": "Password reset for BeelabUserBundle",
    "keywords": ["User management"],
    "homepage": "https://github.com/Bee-Lab",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "beelab/user-bundle": "^2.5",
        "doctrine/orm": "^2.6",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/swiftmailer-bundle": "^3.3",
        "symfony/templating": "^3.4 || ^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.0"
    },
    "autoload": {
        "psr-4": { "Beelab\\UserPasswordBundle\\": "" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
